{
  "name": "kyospe_tms_api",
  "version": "0.31.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/kyospe_tms_api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --testTimeout=30000",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./jest-e2e.json --maxWorkers=1 --detectOpenHandles",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:create:typeorm": "npm run typeorm -- migration:create",
    "migration:generate:typeorm": "npm run typeorm -- migration:generate -d ./libs/database/src/datasource.ts",
    "migration:run:typeorm": "npm run typeorm -- migration:run -d ./libs/database/src/datasource.ts",
    "migration:revert:typeorm": "npm run typeorm -- migration:revert -d ./libs/database/src/datasource.ts",
    "migration:show:typeorm": "npm run typeorm -- migration:show -d ./libs/database/src/datasource.ts",
    "schema:drop:typeorm": "npm run typeorm -- schema:drop -d ./libs/database/src/datasource.ts",
    "schema:sync:typeorm": "npm run typeorm -- schema:sync -d ./libs/database/src/datasource.ts",
    "seed:run:typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@azure/communication-email": "1.0.0",
    "@azure/storage-blob": "^12.15.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "9.4.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "archiver": "^6.0.0",
    "argon2": "^0.30.3",
    "async-lock": "^1.4.0",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.1",
    "cache-manager-ioredis-yet": "^1.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "csv": "^6.3.1",
    "generate-password": "^1.7.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "ms": "^2.1.3",
    "mysql2": "^3.3.0",
    "nestjs-cls": "^3.4.0",
    "nestjs-i18n": "^10.2.6",
    "nestjs-throttler-storage-redis": "^0.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/archiver": "^5.3.2",
    "@types/async-lock": "^1.4.0",
    "@types/compression": "^1.7.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/mime-types": "^2.1.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typeorm-model-generator": "^0.4.6",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "apps/**/*.service.(t|j)s",
      "!apps/api/src/shared/**/*"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@api(|/.*)$": "<rootDir>/apps/api/src/$1",
      "^@batch(|/.*)$": "<rootDir>/apps/batch/src/$1",
      "^@app/app-config(|/.*)$": "<rootDir>/libs/app-config/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1",
      "^@app/google(|/.*)$": "<rootDir>/libs/google/src/$1"
    }
  }
}
